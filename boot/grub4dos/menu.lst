# By gzu-liyujiang on 2019.8.22, last modified on 2019.8.28

default 0
timeout 30

title Windows PE
    echo 'Start Windows PE'
	map /iso/win7pe.iso (hd32)
	map --hook
	chainloader (hd32)

title Windows Boot Manager
    echo 'Start Windows Boot Manager'
    find --set-root /bootmgr
    chainloader /bootmgr
			
title Local System
    echo 'Start Local System'
	rootnoverify (hd0,0)
	makeactive
	chainloader +1

title Restart
	reboot

title Power Off
	halt
